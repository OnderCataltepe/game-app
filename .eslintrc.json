{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "json-format"],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "camelcase": "error",
      "spaced-comment": ["error", "always", { "markers": ["/"] }],
      "quotes": ["error", "single"],
      "no-duplicate-imports": "off",
      "no-unused-vars": "off",
      "react/prop-types": "off"
    }
  }
  